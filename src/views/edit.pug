extends base
    
    
block content 
   h1 Change Title of video
   form(method="POST") 
        input(name="title", placeholder="Video Title", value=video.title, required)
        input(name="description", placeholder="Video Title", value=video.description, required)
        input(name="hashtags", placeholder="Video Title", value=video.hashtags.join(), required)
        input(type="submit", value="save")